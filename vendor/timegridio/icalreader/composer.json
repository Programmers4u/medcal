{
    "name": "timegridio/icalreader",
    "type": "library",
    "description": "iCal reader wrapper for Laravel 5",
    "keywords": [
        "timegridio",
        "icalreader",
        "calendar",
        "ical",
        "parser",
        "api",
        "events",
        "timegrid",
        "time",
        "date"
    ],
    "homepage": "https://github.com/timegridio/icalreader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ariel Vallese",
            "email": "alariva@timegrid.io",
            "homepage": "http://alariva.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "codeclimate/php-test-reporter": "dev-master",
        "laracasts/testdummy": "~2.3",
        "orchestra/testbench": "~3.2.0",
        "nesbot/carbon": "~1.21"
    },
    "autoload": {
        "classmap": [
          "tests/PackageTestCase.php"
        ],
        "psr-4": {
            "Timegridio\\ICalReader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Timegridio\\ICalReader\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
