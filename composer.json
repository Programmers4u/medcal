{
    "name": "programmers4u/medcal",
    "description": "Free, self-hosted online platform for dental service based on timegrido and the Laravel PHP Framework. Medical records with statistics.",
    "homepage": "http://medcal.pl",
    "keywords": ["appointments", "reservations", "booking", "agenda", "marketplace", "time", "date", "calendar","dentist","med"],
    "license": "AGPL",
    "authors": [
        {
            "name": "Marcin Kania",
            "email": "marcin@programmers4u.com",
            "homepage": "http://programmers4u.com",
            "role": "Founder"
        }
    ],
    "support": {
        "issues": "http://medical.programmers4u.com",
        "email": "job@gatesms.eu",
        "wiki": "http://med.programmers4u.com/wiki",
        "source": "http://med.programmers4u.com/source"
    },
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.6.*",
        "anhskohbo/no-captcha": "^3.3",
        "programmers4u/gatesms_sender": "^0.2.0",
        "bassjobsen/bootstrap-3-typeahead": "~4.0",
        "camroncade/timezone": "^0.2.0",
        "creativeorange/gravatar": "^1.0",
        "graham-campbell/markdown": "^10.3",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.4",
        "timegridio/concierge": "dev-master#90e65c",
        "barryvdh/laravel-snappy": "~0.3",
        "patricktalmadge/bootstrapper": "^5.11",
        "ipunkt/laravel-analytics": "^1.6",
        "jenssegers/agent": "^2.6",
        "laracasts/flash": "^3.2",
        "laracasts/utilities": "^3.2",
        "laravel/socialite": "~3.0",
        "propaganistas/laravel-phone": "~2.0",
        "stevebauman/location": "^5.2",
        "torann/geoip": "1.1.*",
        "webpatser/laravel-countries": "^1.5",
        "snowfire/beautymail": "^1.1",
        "spatie/laravel-cookie-consent": "^1.2",
        "alariva/tidiochat": "^2.1",
        "timegridio/icalreader": "dev-master",
        "niklasravnsborg/laravel-pdf": "^4.0",
        "seanstewart/plan-config": "dev-master",
        "fzaninotto/faker": "^1.9",
        "predis/predis": "^1.1",
        "laravel/horizon": "^2.0.*",
        "rollbar/rollbar-laravel": "4.*"
     },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0",
        "laracasts/generators": "^1.1",
        "barryvdh/laravel-debugbar": "^3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/helpers/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]        
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "rollbar/rollbar-laravel"
            ]
        }
    }
}
